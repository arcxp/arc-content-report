# Arc XP Redirect Report Script Configuration
# Copy this file to .env and fill in your actual credentials

# Arc XP Organization ID
ORG_ID=your-org-id-here

# API Bearer Token (get this from Arc XP)
BEARER_TOKEN=your-bearer-token-here

# Website identifier
WEBSITE=your-website-here

# Website domain URL
WEBSITE_DOMAIN=https://www.your-domain-here.com

# Environment (sandbox or production)
ENVIRONMENT=sandbox

# Default date range (optional - can be overridden via command line)
DEFAULT_START_DATE=2020-09-01
DEFAULT_END_DATE=2020-09-30

# Status checking (1 = enabled, 0 = disabled)
DO_404_OR_200=1

# Output filename prefix
OUTPUT_PREFIX=redirects

# Worker configuration
MAX_WORKERS=8
AUTO_OPTIMIZE_WORKERS=1 